    <script>
    if (isNewSession()) {

      window.LANDING_PAGE_URL = 'LANDING_PAGE_URL';
      window.REFERRING_URL = 'REFERRING_URL';
      window.UTM_SOURCE = 'UTM_SOURCE';
      window.UTM_MEDIUM = 'UTM_MEDIUM';
      window.UTM_CAMPAIGN = 'UTM_CAMPAIGN';
      window.UTM_CONTENT = 'UTM_CONTENT';
      window.UTM_TERM = 'UTM_TERM';

      // Set UTM Keys.
      [
        "utm_source",
        "utm_medium",
        "utm_campaign",
        "utm_content",
        "utm_term"
      ].forEach(setFromUrlParam);

      // Set referring and landing url.
      setLandingUrl();
      setReferringURL();
    }


    // Determine if this page load is the first of this session.
    function isNewSession() {
      var tuckerUrlRegex = /^https?:\/\/(w{3}.)?tuckernyc.com/;
      return !(document.referrer.match(tuckerUrlRegex));
    }

    // Store the session originating URL
    // in sessionStorage.
    function setLandingUrl() {
      var value = document.URL;

      if (!Cookies.get(LANDING_PAGE_URL)) {
        Cookies.set(LANDING_PAGE_URL, value);
      }

      //return setIfNotSet("sessionLandingUrl", value);
    }

    // Store the referrer in sessionStorage.
    function setReferringURL() {
      var value = document.referrer;

      if (!Cookies.get(REFERRING_URL)) {
        Cookies.set(REFERRING_URL, value);
      }

      //return setIfNotSet("sessionReferringUrl", value);
    }

    // Store a url parameter to sessionStorage.
    function setFromUrlParam(key) {

      var value = window.getParameterByName(key);

      if (key == "utm_source") {
        Cookies.set(UTM_SOURCE, value);
      }

      if (key == "utm_medium") {
        Cookies.set(UTM_MEDIUM, value);
      }

      // allow UTM_CAMPAIGN to be overwritten
      if (key == "utm_campaign") {
        Cookies.set(UTM_CAMPAIGN, value);
      }

      if (key == "utm_content") {
        Cookies.set(UTM_CONTENT, value);
      }

      if (key == "utm_term") {
        Cookies.set(UTM_TERM, value);
      }
      //return setIfNotSet(key, value);
    }

    // If key is not already set in sessionStorage,
    // set it to the given `value`.
    function setIfNotSet(key, value) {
      var setValue = sessionStorage.getItem(key);
      if (!setValue) {
        sessionStorage.setItem(key, value)
      }
    }

