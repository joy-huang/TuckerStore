  // Populate dataLayer for user
  // initialize dataLayer
  window.dataLayer = window.dataLayer || [];


  // if user has no previously tracked activity
  if (Cookies.get('NO_TRACKED_ACTIVITY')) {
    window.dataLayer.push({
       'DL_NO_TRACKED_ACTIVITY': 1
     });
  }

  // if currently ADDED_TO_CART
  if (Cookies.get('ADDED_TO_CART')) {
        window.dataLayer.push({
       'DL_ADDED_TO_CART': 1
     });
  }

  // if currently INITIATED_CHECKOUT
  if (Cookies.get('INITIATED_CHECKOUT')) {
    window.dataLayer.push({
       'DL_INITIATED_CHECKOUT': 1
     });
  }

  // if currently HAS_ORDERED
  if (Cookies.get('HAS_ORDERED')) {
    window.dataLayer.push({
       'DL_HAS_ORDERED': 1
     });
  }

  // if user is from FB Prospecting
  if (Cookies.get('FB_LEAD')) {
    window.dataLayer.push({
       'DL_FB_LEAD': 1
     });
  }

  // if user is an email subscriber
  if (Cookies.get('EM_SUB')) {
    window.dataLayer.push({
       'DL_EM_SUB': 1
     });
  }
  // if user is a recognized customer
  if (Cookies.get('C_TYPE')) {
	var ctype = Cookies.get('C_TYPE');
    	window.dataLayer.push({
 			'DL_CUST_TYPE': ctype
 		});
  }

  if (Cookies.get('NO_TRACKED_ACTIVITY') || Cookies.get('ADDED_TO_CART') || Cookies.get('INITIATED_CHECKOUT')) {
